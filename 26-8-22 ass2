#include<stdio.h>
#include<string.h>
int main()
{
	char s[10];
	printf("Enter string");
	gets(s);
	int size;
	size=strlen(s);
	int i=0,j=size-1;
	while(i<j/2)
	{
		if(s[i]==s[j])
		{
		printf("Given string is palindrome");
		i++;
		j--;	
	}
	else
	{
		printf("Given string is not palindrome");
		i++;
		j--;
	}
}
puts(s);
return 0;
}
